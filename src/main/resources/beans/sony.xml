<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true"
       >

    <bean id="sonyHeadGold" class="com.hanada.spring.implementation.sony.SonyHead">
        <property name="color" value="Golden"/>
    </bean>
    <bean id="sonyHeadSilver" class="com.hanada.spring.implementation.sony.SonyHead">
        <property name="color" value="Silver"/>
    </bean>
    <bean id="sonyLeg" class="com.hanada.spring.implementation.sony.SonyLeg"/>
    <bean id="sonyHand" class="com.hanada.spring.implementation.sony.SonyHand"/>

</beans>